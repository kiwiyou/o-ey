{
  "name": "__MSG_appName__",
  "short_name": "__MSG_appShortName__",
  "description": "__MSG_appDescription__",
  "version": "0.1.0",
  "manifest_version": 2,
  "__chrome|edge__manifest_version": 3,
  "default_locale": "en",
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "64": "images/64.png",
    "128": "images/128.png",
    "144": "images/144.png",
    "192": "images/192.png",
    "256": "images/256.png"
  },
  "__chrome|edge__action": {
    "browser_style": true,
    "default_area": "navbar",
    "default_popup": "index.html"
  },
  "__firefox|opera|edge|safari__browser_action": {
    "browser_style": true,
    "default_area": "navbar",
    "default_popup": "index.html"
  },
  "background": {
    "__firefox|opera|edge|safari__scripts": ["scripts/background.js"],
    "__chrome|edge__service_worker": "scripts/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.acmicpc.net/problem/*"],
      "js": ["scripts/problem.js"],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": ["storage"]
}
